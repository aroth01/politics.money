<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.polstats.entity-crawler</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/aaronroth/polstats/scripts/entity-crawler.sh</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/aaronroth/polstats</string>

    <key>UserName</key>
    <string>aaronroth</string>

    <key>StandardOutPath</key>
    <string>/tmp/polstats-entity-crawler.log</string>

    <key>StandardErrorPath</key>
    <string>/tmp/polstats-entity-crawler-error.log</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>

    <key>ThrottleInterval</key>
    <integer>30</integer>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PYTHONUNBUFFERED</key>
        <string>1</string>
    </dict>
</dict>
</plist>
